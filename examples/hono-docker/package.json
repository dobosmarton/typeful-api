{
  "name": "hono-docker-example",
  "version": "0.0.1",
  "private": true,
  "description": "Hono + Docker example using typeful-api",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "start": "node dist/index.js",
    "docker:build": "docker build -t hono-docker-example .",
    "docker:run": "docker run -p 3000:3000 hono-docker-example",
    "generate": "pnpm generate:spec && pnpm generate:client",
    "generate:spec": "typeful-api generate-spec --contract ./src/api.ts --out ./openapi.json --title 'Hono Docker Example API' --api-version '1.0.0'",
    "generate:client": "typeful-api generate-client --spec ./openapi.json --out ./src/api.generated.d.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@hono/zod-openapi": "^1.2.0",
    "@typeful-api/core": "workspace:*",
    "@typeful-api/hono": "workspace:*",
    "hono": "^4.11.4",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@typeful-api/cli": "workspace:*",
    "@types/node": "^25.2.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
