{
  "name": "hono-docker-example",
  "version": "0.0.1",
  "private": true,
  "description": "Hono + Docker example using typi",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "start": "node dist/index.js",
    "docker:build": "docker build -t hono-docker-example .",
    "docker:run": "docker run -p 3000:3000 hono-docker-example"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@hono/zod-openapi": "^1.2.0",
    "@typi/core": "workspace:*",
    "@typi/hono": "workspace:*",
    "hono": "^4.11.4",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
