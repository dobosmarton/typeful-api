{
  "name": "express-crud-example",
  "version": "0.0.1",
  "private": true,
  "description": "Express CRUD example using typeful-api",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup src/index.ts --format esm --dts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@typeful-api/core": "workspace:*",
    "@typeful-api/express": "workspace:*",
    "express": "^5.2.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^25.2.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
