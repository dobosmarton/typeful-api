{
  "name": "@typi/hono",
  "version": "0.0.1",
  "description": "Hono adapter for typi - works on Node.js, Cloudflare Workers, Deno, Bun, and more",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@hono/zod-openapi": "^1.2.0",
    "@typi/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.39.2",
    "hono": "^4.11.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "hono": ">=4.3.6",
    "zod": ">=4.0.0"
  },
  "keywords": [
    "openapi",
    "typescript",
    "hono",
    "api",
    "typesafe",
    "multi-runtime",
    "nodejs",
    "cloudflare-workers",
    "deno",
    "bun"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/martondobos/typi.git",
    "directory": "packages/hono"
  }
}
