{
  "name": "@typefulapi/cli",
  "version": "0.1.0",
  "description": "CLI for generating OpenAPI specs and client types from typefulapi contracts",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "typefulapi": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@typefulapi/core": "workspace:*",
    "cac": "^6.7.14",
    "jiti": "^2.4.2",
    "openapi-typescript": "^7.10.1",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "eslint": "^9.39.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "zod": "^4.3.5"
  },
  "keywords": [
    "openapi",
    "typescript",
    "cli",
    "codegen",
    "typesafe"
  ],
  "author": "Marton Dobos",
  "license": "MIT",
  "homepage": "https://github.com/dobosmarton/typefulapi#readme",
  "bugs": {
    "url": "https://github.com/dobosmarton/typefulapi/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dobosmarton/typefulapi.git",
    "directory": "packages/cli"
  }
}
